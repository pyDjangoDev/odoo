<odoo>

    <record model="ir.ui.view" id="estate_estate_property_offer_view_tree">
        <field name="name">Property Offer List</field>
        <field name="model">estate.estate.property.offer</field>
        <field name="priority" eval="16" />
        <field name="arch" type="xml">
            <tree string="Property Offer List">
                <field name="price" />
                <field name="partner_id" />
                <field name="validity" />
                <field name="date_deadline" />
                <field name="status" />
            </tree>
        </field>
    </record>

    <record model="ir.ui.view" id="estate_estate_property_offer_view_form">
        <field name="name">Property Offer Form</field>
        <field name="model">estate.estate.property.offer</field>
        <field name="priority" eval="16" />
        <field name="arch" type="xml">
            <form string="Property Form">
                <sheet>
                    <group>
                        <field name="price" />
                        <field name="partner_id" />
                        <field name="validity" />
                        <field name="date_deadline" />
                        <field name="status" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>